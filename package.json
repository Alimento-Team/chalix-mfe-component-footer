{
  "name": "@chalix/frontend-component-footer",
  "version": "1.0.0",
  "description": "Footer component for use when building Chalix Open edX frontend applications",
  "main": "index.js",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com",
    "@alimento-team:registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "scripts": {
    "build": "echo 'Skipping build - using pre-built components from dist/ folder'",
    "build:dev": "fedx-scripts webpack --mode=development",
    "prepare": "echo 'Skipping prepare step'",
    "i18n_extract": "fedx-scripts formatjs extract",
    "lint": "fedx-scripts eslint --ext .js --ext .jsx .",
    "lint:fix": "fedx-scripts eslint --fix --ext .js --ext .jsx .",
    "snapshot": "fedx-scripts jest --updateSnapshot",
    "start": "fedx-scripts webpack-dev-server --progress",
    "start:with-theme": "paragon install-theme && npm start && npm install",
    "test": "fedx-scripts jest --coverage",
    "test:watch": "npm run test -- --watch"
  },
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
